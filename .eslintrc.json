{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["github"],
  "extends": ["plugin:github/recommended"],
  "rules": {
    "i18n-text/no-en": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
